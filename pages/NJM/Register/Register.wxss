.Register {
  margin: 70px 0 30px 0;
  width: 80%;
  padding: 12px 0 12px 0;
  text-align: center;
  background-color: #F4B183;
  border-radius: 0.1cm;
  color: white;
  border: 1px solid black;
}

#register {
  width: 100%;
  margin: 10px 0 10px 0;
}

.Register_btn {
  margin-top: 10px;
  width: 80%;
}

.Bill {
  width: 100%;
  text-align: center;
}

.Receipt {
  width: 100%;
  text-align: center;
}

#button {
  width: 100%;
  border: 1px solid black;
}

#image {
  width: 100%;
  padding-top: 8px;
  max-height: 90px;
}

.select_box {
  background-color: #eee;
  width: 24%;
  position: absolute;
  top: 40px;
  z-index: 1;
  overflow: hidden;
  text-align: center;
  animation: myfirst 0.5s;
  margin-left: 34%;
}

@keyframes myfirst {
  from {
      height: 0px;
  }
  to {
      height: 52px;
  }
}

.select_one {
  height: 25px;
  line-height: 25px;
  border-bottom: 1px solid #ccc;
}

.list-msg2 {
  margin-top: 10px;
  width: 24%;
  text-align: center;
  height: 23px;
  border: 1px solid #ccc;
  margin-left: 34%;
  position: absolute;
  background-color: white;
  padding-top: 5px;
}